{% include "tabbed-carousel" %}


{% schema %}
  {
    "name": "Tabbed section",
    "settings": [
		{
			"type": "text",
			"id"  : "title",
			"label": "Title"
		},
		{
			"type": "text",
			"id"  : "subtitle",
			"label": "Subtitle"
		},
		{
			"type": "text",
			"id"  : "tab_title",
			"label": "Section Title"
		}
	],
	"blocks": [
		{
			"type": "tab",
			"name": "Collection",
			"settings":[
				{
					"type": "text",
                    "id"  : "title",
                    "label": "Title"
				},
				{
					"type": "collection",
                    "id"  : "collection",
                    "label": "Collection"
				}
			]
		}
	],
     "presets":[ 
        { 
           "name":"Tabbed collection",
           "category":"collection"
        }
     ]
  }
{% endschema %}

